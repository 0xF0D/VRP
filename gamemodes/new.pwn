					else if(PlayerInfo[playerid][pMRInections]==21 || PlayerInfo[playerid][pLRInections]==21) { format(stringer1,sizeof(stringer1), "<< Офицер %s арестовал %s >>", PlayerInfo[playerid][pNames], PlayerInfo[params[0]][pNames]); SendClientMessageToAll(COLOR_LIGHTRED, stringer1); }
