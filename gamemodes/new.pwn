					PlayersHaul[tmpcar-benzovoz[0]][sLoad] -= benz;
				}
				else
				{
					SBizzInfo[i][sBenz] += PlayersHaul[tmpcar-benzovoz[0]][sLoad];
					PlayerInfo[playerid][pCash] += SBizzInfo[i][sPrice]*PlayersHaul[tmpcar-benzovoz[0]][sLoad];
