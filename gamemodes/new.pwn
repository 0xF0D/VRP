SellBusiness(playerid)
{
	if(IsPlayerInRangeOfPoint( playerid, 10.0, BizInfo[PlayerInfo[playerid][pPbiskey]][ EnterX ], BizInfo[PlayerInfo[playerid][pPbiskey]][ EnterY ], BizInfo[PlayerInfo[playerid][pPbiskey]][ EnterZ ] ) )
	{
