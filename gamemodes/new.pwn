		acc_str_strcat(update, sizeof(update), "bOwner", "None");
		acc_int_strcat(update, sizeof(update), "bLock", 0);
		acc_int_strcat(update, sizeof(update), "bPribil", 0);
		strdel(update, strlen(update)-1, strlen(update));
		format(stringerg,sizeof(stringerg)," WHERE `bID` = '%d'",PlayerInfo[playerid][pPbiskey]);
		strcat(update, stringerg);
		mysql_tquery(connects, update);
		GiveMoney(playerid,BizInfo[PlayerInfo[playerid][pPbiskey]][bPrice]);
		UpdateBizInfo (PlayerInfo[playerid][pPbiskey]);
		SCMF(playerid,COLOR_WHITE,"Вы продали свой бизнес за {FFFF00}%d",BizInfo[PlayerInfo[playerid][pPbiskey]][bPrice]);
