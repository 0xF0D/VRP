				SCMF(playerid,COLOR_GREY,"[ MySql ] Аккаунт %s не является вашим членом фракции.",name);
				return true;
			}
		}
		mysql_format(connects, stringer, MAX_SQL, "UPDATE `accounts` SET `pMRInections` = '0', `pRKInections` = '0' WHERE `Name` = '%s'", name); mysql_tquery(connects, stringer);
		SCMF(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно уволен из фракции.", name);
	}
	else SCMF(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",name);
