stock PayDay()
{
	if(BizWarTime==0) BizHourTime--;
	for(new f = 0; f < sizeof(FrakCD); f++) { if(FrakCD[f] > 0) { if(FracVoina[f] != 1) { FrakCD[f]--; } } }
	for(new i =0;i<sizeof(VladGzone);i++) VladGzone[i] = 0;
	FracBank[0][fGrove] += VladGzone[2]*200;
	FracBank[0][fVagos] += VladGzone[1]*200;
	FracBank[0][fRifa] += VladGzone[4]*200;
	FracBank[0][fAztek] += VladGzone[3]*200;
	FracBank[0][fBallas] += VladGzone[0]*200;
	SaveBankFrac();
	foreach(i)
	{
		if(PlayerInfo[i][bAdmin] > 0 && PlayerInfo[i][pMRInections] > 0)
		{
			PlayerInfo[i][pLRInections] = 0;
			PlayerInfo[i][pMRInections] = 0;
			PlayerInfo[i][pRKInections] = 0;
		}
		if(PlayerInfo[i][pWanted] >= 1)
		{
			WantedLevel[i] -= 1;
			PlayerInfo[i][pWanted] -=1;
			SetPlayerWantedLevel(i, PlayerInfo[i][pWanted]);
			UpdatePlayerData(i, "pWanted", PlayerInfo[i][pWanted]);
		}
		new exp = PlayerInfo[i][pExp],nxtlevel = PlayerInfo[i][pLevel]+1,expamount = nxtlevel*levelexp;
		if(exp >= expamount)
		{
			PlayerInfo[i][pLevel] += 1;
			PlayerInfo[i][pExp] = 0;
			SCM(i,COLOR_ORANGE,"Поздравляем! Ваш игровой уровень повышен!");
			SetPlayerScore(i, PlayerInfo[i][pLevel]);
		}
		if(armmatbi < 0) armmatbi = 0;
		else if(armmatsf < 0) armmatsf = 0;
		else if(kazna < 0) kazna = 0;
		else if(lspdmatbi < 0) lspdmatbi = 1000;
		else if(sfpdmats < 0) sfpdmats = 1000;
		else if(lvpdmats < 0) lvpdmats = 1000;
		else if(fbimats < 0) fbimats = 1000;
		else if(yakuzamats < 0) yakuzamats = 1000;
		else if(rmmats < 0) rmmats = 1000;
		else if(lcnmats < 0) lcnmats = 1000;
		else if(rifamatbi < 0) rifamatbi = 1000;
		else if(ballasmatbi < 0) ballasmatbi = 1000;
		else if(groovmatbi < 0) groovmatbi = 1000;
		else if(vagosmatbi < 0) vagosmatbi = 1000;
		else if(aztekmatbi < 0) aztekmatbi = 1000;
		new moneys = 1+random(10000);
		switch(PlayerInfo[i][pMRInections])
		{
		case 1,10,21,25:{ PlayerInfo[i][pPayCheck] += 2000*PlayerInfo[i][pRKInections]+moneys; } // ЗП полиции, 2к умноженое на ранг + рандомные цифры
		case 3,19,27:{ PlayerInfo[i][pPayCheck] += 1500*PlayerInfo[i][pRKInections]+moneys; } // Зп армии, 1.5к умноженое на ранг + рандомные цифры
		case 2:{ PlayerInfo[i][pPayCheck] += 3000*PlayerInfo[i][pRKInections]+moneys; } // Зп ФБР, 3к умноженое на ранг + рандомные цифры
		case 7:{ PlayerInfo[i][pPayCheck] += 4000*PlayerInfo[i][pRKInections]+moneys; } // Зп Мэрия, 4к умноженое на ранг + рандомные цифры
		case 4,22:{ PlayerInfo[i][pPayCheck] += 4000*PlayerInfo[i][pRKInections]+moneys; } // Зп МинЗдрав, 4к умноженое на ранг + рандомные цифры
		case 11:{ PlayerInfo[i][pPayCheck] += 1000*PlayerInfo[i][pRKInections]+moneys; } // Зп инструкторов, 4к умноженое на ранг + рандомные цифры
		}
		PlayerInfo[i][pExp]++;
		PlayerPlaySound(i, 5203, 0.0, 0.0, 10.0);
		new checks = PlayerInfo[i][pPayCheck];
		if(PlayerInfo[i][pDRInections] >=1) GameTextForPlayer(i, "PayDay", 5000, 1);
		else GameTextForPlayer(i, "~w~PayDay", 5000, 1);
		if(PlayerInfo[i][pDRInections] >=1) SCM(i, 0xeac700aa,  "--------===[ VIP КЛИЕНТ БАНКА SA ]===-------");
		else SCM(i, COLOR_GRAD1, "----------===[ КЛИЕНТ БАНКА SA ]===---------");
		SCMF(i,0xFF8C37AA,"______Налог государству: {FFD300}%d{FF8C37} вирт______", nalog);
		PlayerInfo[i][pBank] -= nalog;
		kazna +=nalog;
		SaveMeria();
		if(PlayerInfo[i][pDRInections] >=1)
		{
			SCMF(i,0xFF8C37AA," Счет за телефон: {FFD300}-%d {FF8C37} вирт",PlayerInfo[i][pMobile]);
			SCM(i, 0xeac700aa, " ");
			SCMF(i,0xFF8C37AA," Зарплата: {FFD300}%d {FF8C37}вирт", checks);
			PlayerInfo[i][pBank] += checks;
			SCMF(i,0xeac700aa," Текущий баланс: {FFD300}%d {eac700}вирт", PlayerInfo[i][pBank]);
			PlayerInfo[i][pZakonp] += 1;

			PlayerInfo[i][pBank] += PlayerInfo[i][pMobile];
			PlayerInfo[i][pMobile] = 0;
			PlayerInfo[i][pVipRespect]++;
