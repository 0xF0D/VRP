publics: OffBan(playerid,tmp[],reason[])
{
	new rows = cache_num_rows();
	if(rows) return SCM(playerid, COLOR_WHITE, "Игрок заблокирован!");
	if(strcmp(PlayerInfo[playerid][pNames], FD1, true) && strcmp(PlayerInfo[playerid][pNames], FD2, true) && strcmp(PlayerInfo[playerid][pNames], FD3, true) && strcmp(PlayerInfo[playerid][pNames], FD4, true)) SCM(playerid, COLOR_WHITE, "Основателей запрещено банить");
	new data[16],year, month, day,unbandate;
	getdate(year, month, day);
	format(data, 16, "%02d.%02d.%04d", day, month, year);
	unbandate = gettime() + 7*86400;
	mysql_format(connects, stringer1, BIG_SIZE, "INSERT INTO `table_ban` (`Name_Admin`, `Name_Player`, `Ban_Data`, `Unban_Data`, `Ban_Reason`) VALUES ('%s', '%s', '%s', '%d', '%s')", PlayerInfo[playerid][pNames], tmp, data, unbandate, reason);
	mysql_tquery(connects, stringer1);
	new year1, month1, day1, hour1, minute1, second1;
	getdate(year1, month1, day1);
	gettime(hour1, minute1, second1);

	if (minute1 < 10)
	{
		format(stringer1, MAX_STR, "offBan[забанил: %s][забанен: %s][Причина: %s][%d/%d/%d  %d:0%d]",PlayerInfo[playerid][pNames], tmp, reason, day1, month1, year1, hour1, minute1);
		ABroadCast(COLOR_LIGHTRED,stringer1,1);
	}
	else
	{
		format(stringer1, MAX_STR, "offBan[забанил: %s][забанен: %s][Причина: %s][%d/%d/%d  %d:%d]",PlayerInfo[playerid][pNames], tmp, reason, day1, month1, year1, hour1, minute1);
		ABroadCast(COLOR_LIGHTRED,stringer1,1);
	}
	return true;
}


