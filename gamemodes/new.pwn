publics: LoadSBizz()
{
	new i = 0;
	new string2[256];
	if(cache_num_rows() > 0)
	{
		for(new he = 0;he < cache_num_rows(); he++)
		{
			SBizzInfo[he][sID] = cache_get_field_content_int (he, "sID");
			SBizzInfo[he][sOwned] = cache_get_field_content_int (he, "sOwned");
			cache_get_field_content(he, "sOwner", SBizzInfo[he][sOwner], connects, strlen(SBizzInfo[he][sOwner]));
			cache_get_field_content(he, "sMessage", SBizzInfo[he][sMessage], connects, strlen(SBizzInfo[he][sMessage]));
			SBizzInfo[he][sEntranceX] = cache_get_field_content_float (he, "sEntranceX");
			SBizzInfo[he][sEntranceY] = cache_get_field_content_float (he, "sEntranceY");
			SBizzInfo[he][sEntranceZ] = cache_get_field_content_float (he, "sEntranceZ");
			SBizzInfo[he][sBuyPrice] = cache_get_field_content_int (he, "sBuyPrice");
			SBizzInfo[he][sLocked] = cache_get_field_content_int (he, "sLocked");
			SBizzInfo[he][sPrice] = cache_get_field_content_int (he, "sPrice");
			SBizzInfo[he][sBenz] = cache_get_field_content_int (he, "sBenz");
			SBizzInfo[he][sPriceBenz] = cache_get_field_content_int (he, "sPriceBenz");
			SBizzInfo[he][sTill] = cache_get_field_content_int (he, "sTill");

			i++;
			if(SBizzInfo[he][sOwned] == 0)
			{
				//
				SBizzInfo[he][sBuyPrice] = 1000000;
				SBizzInfo[he][sOwned] = 0;
				SBizzInfo[he][sLocked] = 1;
				SBizzInfo[he][sPrice] = 2500;
				SBizzInfo[he][sBenz] = 2500;
				SBizzInfo[he][sPriceBenz] = 2500;
				SBizzInfo[he][sTill] = 100000;
				//
				format(string2, sizeof(string2), "{FFFFFF}Заправка{FFD300}(%d){FFFFFF} продается.\n{FFD300}%s{FFFFFF}\nЦена: {FFD300}%d\n{FFFFFF}Введите {FFD300}`/buyfuel`",SBizzInfo[he][sID],SBizzInfo[he][sMessage],SBizzInfo[he][sBuyPrice]);
			}
			else
			{
				if(SBizzInfo[he][sLocked] == 0) format(string2, sizeof(string2), "{FFFF00}%s\n{FFFFFF}Владелец: {32CD32}%s\nБензин: %d/10000\nСтоимость: %d вирт.\n\n{FFFFFF}[{32CD32}Заправка работает{FFFFFF}]",SBizzInfo[he][sMessage],SBizzInfo[he][sOwner],SBizzInfo[he][sBenz],SBizzInfo[he][sPriceBenz]);
				else format(string2, sizeof(string2), "{FFFF00}%s\n{FFFFFF}Владелец: {32CD32}%s\nБензин: %d/10000\nСтоимость: %d вирт.\n\n{FFFFFF}[{FFD300}Заправка не работает{FFFFFF}]",SBizzInfo[he][sMessage],SBizzInfo[he][sOwner],SBizzInfo[he][sBenz],SBizzInfo[he][sPriceBenz]);
			}
			SBizzInfo[he][sLabel] = Create3DTextLabel(string2,COLOR_YELLOW,SBizzInfo[he][sEntranceX], SBizzInfo[he][sEntranceY], SBizzInfo[he][sEntranceZ],20.0,0,1);
			SBizzInfo[he][sIcon] = CreateDynamicMapIcon(SBizzInfo[he][sEntranceX], SBizzInfo[he][sEntranceY], SBizzInfo[he][sEntranceZ], 42, COLOR_WHITE, 0, -1, -1, 50.0);
		}
		printf("[Загрузкa]: LoadSBizz | Загружено %d",i);
	}
	else printf("сБизнесов не загружены.");
	return true;
}

stock SaveSBizz(houseid)
