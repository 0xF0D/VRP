publics: Check_Ban_List(playerid,Name_Player[],Number_Ban)
{
	new
	ADMIN_ISSUING_BAN[24],
	NAME_PLAYER[24],
	BAN_DATA[32],
	UNBAN_DATA,
	REASON_FOR_THE_BAN[50]
	;
	new rows = cache_num_rows();
	if(rows)
	{
		cache_get_field_content(0, "Name_Admin",ADMIN_ISSUING_BAN, connects, 24);
		cache_get_field_content(0, "Name_Player",NAME_PLAYER, connects, 24);
		cache_get_field_content(0, "Ban_Data",BAN_DATA, connects, 32);
		cache_get_field_content(0, "Ban_Reason",REASON_FOR_THE_BAN, connects, 50);
		UNBAN_DATA = cache_get_field_content_int(0, "Unban_Data");
		if(UNBAN_DATA > gettime())
		{
			format(stringer, BIG_SIZE, "{FF6347}Аккаунт: %s\nДата блокировки: %s\nАдминистратор: %s\nПричина: %s\n\nДата разблокировки: %s",Name_Player,BAN_DATA,ADMIN_ISSUING_BAN,REASON_FOR_THE_BAN, date("%dd/%mm/%yyyy в %hh:%ii",UNBAN_DATA));
			SPD(playerid,91,0,"Аккаунт заблокирован:",stringer,"Закрыть","");
			if(Number_Ban == 2) SetTimerEx("PlayerKick",500,false,"d", playerid,0);
			return true;
		}
		else
		{
			mysql_format(connects, stringer, MAX_SQL, "DELETE FROM `table_ban` WHERE `Name_Player` = '%s'",Name_Player);
			mysql_tquery(connects, stringer);
			SetPVarInt(playerid,"table_ban",1);
			OnPlayerLoginA(playerid);
			return true;
		}
	}
	else
	{
		if(Number_Ban == 1) return SCM(playerid, COLOR_WHITE,"Игрок не был заблокирован!");
		SetPVarInt(playerid,"table_ban",1);
		OnPlayerLoginA(playerid);
	}
	return true;
}
