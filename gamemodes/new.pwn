{
	new plyWeapons[12] = 0;
	new plyAmmo[12] = 0;
	for(new slot = 0; slot != 12; slot++){
		new wep, ammo;
		AC_GetPlayerWeaponData(playerid, slot, wep, ammo);
		if(wep != weaponid && ammo != 0) AC_GetPlayerWeaponData(playerid, slot, plyWeapons[slot], plyAmmo[slot]); }
