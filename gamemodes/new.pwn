CMD:sban(playerid,params[])
{
	if(PlayerInfo[playerid][bAdmin] < 13) return true;
	if(sscanf(params,"ds[100]",params[0],params[1])) return	SCM(playerid, COLOR_WHITE, " Введите: /sban [id] [причина]");
	//	if(anti[playerid] == true) return SCM(playerid, COLOR_LIGHTRED, "[Анти-Наказание] Вы были сняты с поста администратора (Если произошло по ошибке сделайте скриншот)"),DelAdmin(playerid,PlayerInfo[playerid][pNames]);
	//	if(AntiBan[playerid] >= 200) return SCM(playerid,COLOR_WHITE,"[Анти-перебан] Вы были сняты с поста администратора (Если произошло по ошибке сделайте скриншот)"),DelAdmin(playerid,PlayerInfo[playerid][pNames]);
	Reklama(playerid, params[1]);
	if(!IsPlayerConnected(params[0])) return SCM(playerid, COLOR_GREY, "Ошибка: Человек не найден!");
	if(PlayerInfo[params[0]][bAdmin] > 0 && PlayerInfo[playerid][bAdmin] < 12) return SCM(playerid, COLOR_GRAD2, "Администратор не может быть забанен!");
	antiwarn[playerid] += 8;
	if(antiwarn[playerid] >= 30 && anti[playerid] == false) anti[playerid] = true, antiwarn[playerid] = 300;
	new data[16],unbandate;
	new day, month, year;
	getdate(year, month, day);
	format(data, 16, "%02d.%02d.%04d", day, month, year);
	unbandate = gettime() + 7*86400;
	mysql_format(connects, stringer1, BIG_SIZE, "INSERT INTO `table_ban` (`Name_Admin`, `Name_Player`, `Ban_Data`, `Unban_Data`, `Ban_Reason`) VALUES ('%s', '%s', '%s', '%d', '%s')", PlayerInfo[playerid][pNames], PlayerInfo[params[0]][pNames], data, unbandate, params[2]);
	mysql_tquery(connects, stringer1);
	AntiBan[playerid] += 30;
	format(stringer1,MAX_STR, "Sban[забанил: %s][забанен: %s][причина: %s]",PlayerInfo[playerid][pNames], PlayerInfo[params[0]][pNames], params[1]);
	ABroadCast(COLOR_LIGHTRED,stringer1,1);
	format(stringer1,MAX_STR, "    Nik  [%s]    R-IP [%s]    L-IP  [%s]    IP  [%s]",PlayerInfo[params[0]][pNames],PlayerInfo[params[0]][pIpReg],PlayerInfo[params[0]][pIpReg],PlayerInfo[params[0]][pIP]);
	ABroadCast(COLOR_LIGHTRED,stringer1,1);
	KickEx(params[0],34);
	return true;
}
CMD:capture(playerid)
{
	if (!IsAGang(playerid)) return SCM(playerid,COLOR_WHITE, "Вам не доступна эта функция");
	if(PlayerInfo[playerid][pRKInections] < 7) return SCM(playerid, COLOR_GRAD1, "Вам нужен 7 ранг");
	if(FrakCD[PlayerInfo[playerid][pMRInections]] > 0) return SCM(playerid,COLOR_WHITE,"Ваша банда уже воевала за территорию.");
	if(PlayerInfo[playerid][pMRInections] == 15 && groves == 1) return SCM(playerid, COLOR_GREY, "Администрация запретила Вашей банде воевать за территории");
	if(PlayerInfo[playerid][pMRInections] == 13 && vagoss == 1) return SCM(playerid, COLOR_GREY, "Администрация запретила Вашей банде воевать за территории");
	if(PlayerInfo[playerid][pMRInections] == 12 && ballass == 1) return SCM(playerid, COLOR_GREY, "Администрация запретила Вашей банде воевать за территории");
	if(PlayerInfo[playerid][pMRInections] == 18 && rifas == 1) return SCM(playerid, COLOR_GREY, "Администрация запретила Вашей банде воевать за территории");
	if(PlayerInfo[playerid][pMRInections] == 17 && aztecas == 1) return SCM(playerid, COLOR_GREY, "Администрация запретила Вашей банде воевать за территории");
	new captureghetto = 0;
	new t; gettime(t);
	if(t < 12 || t > 22 ) return SCM(playerid, COLOR_GREY, "Использовать команду можно только с 12:00 до 23:00");
	for(new i = 0; i < sizeof(GZInfo); i++)
	{
		if(PlayerToKvadrat(playerid,GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]) && GZInfo[i][ZoneOnBattle] == 0)
		{
			if(PlayerInfo[playerid][pMRInections] == GZInfo[i][gFrakVlad]) return SCM(playerid,COLOR_WHITE,"Необходимо находится на территори которую хотите завоевать");
			if(FrakCD[GZInfo[i][gFrakVlad]] > 0) return SCM(playerid,COLOR_WHITE,"Банда на которую вы хотите напасать уже воевала в этом часу");
			if(!IsPlayerInBandOnline(GZInfo[i][gFrakVlad])) return SCM(playerid,COLOR_WHITE, "Банда на которую вы собираетесь напасть нет в сети!");
			captureghetto++;
			GZInfo[i][gTime] = 600;
			GetPlayer2DZone(playerid,GZInfo[i][GZName],30);
			GZInfo[i][gNapad] = PlayerInfo[playerid][pMRInections];
			format(stringer1,112,"%s начали захват территории банды %s в районе %s",GFrac(PlayerInfo[playerid][pMRInections]),GFrac(GZInfo[i][gFrakVlad]),GZInfo[i][GZName]);
			SendBand(PlayerInfo[playerid][pMRInections],GZInfo[i][gFrakVlad],COLOR_ORANGE,stringer1);
			format(stringer,144,"{"#cGRAY"}[A]{"#cORANGE"} Банда %s начала захват территории %s {"#cGRAY"}(Захват начал: %s)",GetGangName(PlayerInfo[playerid][pMRInections]),GetGangName(GZInfo[i][gFrakVlad]),PlayerInfo[playerid][pNames]);
			ABroadCast(COLOR_ORANGE,stringer,1);
			GangZoneFlashForAll(GZInfo[i][gID],GetGangColor(GZInfo[i][gNapad]));
			GZInfo[i][ZoneOnBattle] = 1;
			FracCD(PlayerInfo[playerid][pMRInections],GZInfo[i][gFrakVlad],1);
			GZInfo[i][gzTimer] = SetTimerEx("GzCheck",1000,1,"iii",i,PlayerInfo[playerid][pMRInections],GZInfo[i][gFrakVlad]);
			break;
		}
	}
	if(captureghetto == 0) return SCM(playerid,COLOR_WHITE,"Необходимо находится на территори которую хотите завоевать");
	return true;
}
