					SCM(playerid, COLOR_YELLOW2, ":::: Доставка ::::");
					PlayersHaul[tmpcar-benzovoz[0]][sLoad] = 0;
				}
				format(stringer1,sizeof(stringer1), "{FFFF00}%s\n{FFFFFF}Владелец: {32CD32}%s\nБензин: %d/10000\nСтоимость: %d вирт.\n\n{FFFFFF}[{32CD32}Заправка работает{FFFFFF}]",SBizzInfo[i][sMessage],SBizzInfo[i][sOwner],SBizzInfo[i][sBenz],SBizzInfo[i][sPriceBenz]);
