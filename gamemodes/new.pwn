publics:TimerHospital(playerid)
{
	if(HospTime[playerid] == true)
	{
		new Float: healths;
		GetPlayerHealth(playerid,healths);
		if(healths < 100.0 && PlayerInfo[playerid][pHospital] == 1)
		{
			SetPlayerHealth(playerid,healths + 5.0);
			PlayerHealth[playerid] += 5.0;
			GameTextForPlayer(playerid,"~g~+ ~w~5 HP",2000,1);
		}
		else
		{
			for(new i = 0; i < MAX_HOSPITAL; i++)
			{
				if(strcmp(PlayerInfo[playerid][pNames],HospitalInfo[i][hName],true)) return true;
				strmid(HospitalInfo[i][hName],"None",1,4);
				HospitalInfo[i][hStatus] = 0;
				format(stringer,200,"{FFFFFF}Койка - {FF7800}№%d{FFFFFF}\nКойка свободна\nНажмите клавишу {FF7800}N{FFFFFF} чтобы занять.",HospitalInfo[i][hospID]);
				UpdateDynamic3DTextLabelText(HospitalInfo[i][hLabelHosp],0x317CDFFF,stringer);
			}

			DeletePVar(playerid,"HospRecover");
			PlayerInfo[playerid][pHospital] = 0;
			TogglePlayerControllable(playerid,true);
			ClearAnimations(playerid);
			KillTimer(HospTimer[playerid]);
			HospTime[playerid] = false;

			SCM(playerid, COLOR_ORANGE, "Ваше лечение успешно завершено.");
			SCM(playerid, COLOR_ORANGE, "Теперь Вы можете выйти из больницы. Всего доброго.");
		}
	}
	else { KillTimer(HospTimer[playerid]); HospTime[playerid] = false; }
	return true;
}

stock GetSpeed(playerid)
{
	new Float:STt[4];
	if(IsPlayerInAnyVehicle(playerid))
	GetVehicleVelocity(GetPlayerVehicleID(playerid),STt[0],STt[1],STt[2]);
	else GetPlayerVelocity(playerid,STt[0],STt[1],STt[2]);
	STt[3] = floatsqroot(floatpower(floatabs(STt[0]), 2.0) + floatpower(floatabs(STt[1]), 2.0) + floatpower(floatabs(STt[2]), 2.0)) * 100.3;
	return floatround(STt[3]);
}
publics: OnPlayerCheat(playerid,code)
{
	if(GetPVarInt(playerid, "zvonit") == 1) return 1;
	if(GetPVarInt(playerid, "PLAYERANIM") == 1) return 1;
	if(PlayerInfo[playerid][bAdmin] > 0) return true;
	if(GetPVarInt(playerid,"AntiKickDom") == 1) return 1;
	if(GetPVarInt(playerid, "checksalon") == 1) return 1;

	new strrcheat[42+10+145];
	if(code < 10)
	format(strrcheat, 42+10+145, "{FFFFFF}R_Protect - Anti Cheat\n\n{FF9200}Вы были кикнуты по подозрении в читерстве (#00%d)\n\nЕсли Вы были кикнуты ложно, сделайте скриншот (F8) и сообщите в технический раздел форума.",code);
	else
	format(strrcheat, 42+10+145, "{FFFFFF}R_Protect - Anti Cheat\n\n{FF9200}Вы были кикнуты по подозрении в читерстве (#0%d)\n\nЕсли Вы были кикнуты ложно, сделайте скриншот (F8) и сообщите в технический раздел форума.",code);
	ShowPlayerDialog(playerid, 110000, DIALOG_STYLE_MSGBOX, "{FFD300}Rem.Project[AC]", strrcheat , "Выход","");
	SetTimerEx("PlayerKick",100,false,"d", playerid);
	return 1;
}

stock GetNumberOfPlayersOnThisIP( test_ip [ ] )
{
	new against_ip [ 32+1 ],ip_count = 0;
	foreach( x )
	{
		if( !IsPlayerConnected ( x ) ) continue;
		GetPlayerIp( x , against_ip , 32 );
		if( !strcmp ( against_ip , test_ip ) ) ip_count++;
	}
	return ip_count;
}
