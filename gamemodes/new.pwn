CMD:ban(playerid, params[])
{
	if(PlayerInfo[playerid][bAdmin] < 12) return true;
	if(sscanf(params, "dds[256]", params[0], params[1], params[2])) return SCM(playerid, COLOR_WHITE, "Введите: /ban [id] [дни] [причина]");
	if(params[0] < 0 || params[0] > MAX_PLAYERS) return SCM(playerid, COLOR_GRAD2, "Неверный ид игрока");
	if(params[1] < 1 || params[1] > 30) return SCM(playerid, COLOR_GRAD2, "Забанить можно от 1 до 30 дней.");
	if(AntiBan[playerid] >= 200) return SCM(playerid,COLOR_WHITE,"[Анти-перебан] Вы были сняты с поста администратора (Если произошло по ошибке сделайте скриншот)"),DelAdmin(playerid,PlayerInfo[playerid][pNames]);
	if(!strcmp(PlayerInfo[playerid][pNames], FD1, false) && !strcmp(PlayerInfo[playerid][pNames], FD2, false) && !strcmp(PlayerInfo[playerid][pNames], FD3, false)) return SCM(playerid, COLOR_WHITE, "Основателей запрещено банить");
	if(PlayerInfo[params[0]][bAdmin] > PlayerInfo[playerid][bAdmin]) return SCM(playerid, COLOR_GRAD2, "Администратора нельзя забанить");
	if(!IsPlayerConnected(params[0])) return SCM(playerid, COLOR_WHITE, "Игрок оффлайн");
	Reklama(playerid, params[2]);
	for(new i;i < params[1];i++)
	{
		new a[2],c=0;
		for(new j = 0;j < params[1];j++) //5
		{
			strmid(a,params[1],i+j,i+j+1,2);
			if('0' <= a[0] <= '9')c+=1;
		}
		if(c == 9)
		{
			SCM(playerid, 0xFFFFFFAA, " Слишком много цифр");
			SCM(playerid, 0xFFFFFFAA, " Идет анализ твоего сообщения, ты снят с администраторских прав.");
			format(stringer,MAX_STR," Внимание: %s[%d] пытался отправить текст: %s",PlayerInfo[playerid][pNames], playerid, params[1]);
			ABroadCast(COLOR_YELLOW,stringer,1);
			DelAdmin(playerid,PlayerInfo[playerid][pNames]);
			return true;
		}
	}
	format(stringer, MAX_STR, "%s %s заблокировал игрока %s на %d дней. Причина: %s",GetAdmin(PlayerInfo[playerid][bAdmin]),PlayerInfo[playerid][pNames],PlayerInfo[params[0]][pNames], params[1],params[2]);
	SendClientMessageToAll(COLOR_LIGHTRED,stringer);

	new ip[16];
	GetPlayerIp(params[0],ip,sizeof(ip));
	antiwarn[playerid] += 8;
	if(antiwarn[playerid] >= 30 && anti[playerid] == false) anti[playerid] = true, antiwarn[playerid] = 300;
	new data[16],unbandate;
	new day, month, year;
	getdate(year, month, day);
	format(data, 16, "%02d.%02d.%04d", day, month, year);
	unbandate = gettime() + params[1]*86400;
	mysql_format(connects, stringer1, sizeof(stringer1), "INSERT INTO `table_ban` (`Name_Admin`, `Name_Player`, `Ban_Data`, `Unban_Data`, `Ban_Reason`) VALUES ('%s', '%s', '%s', '%d', '%s')", PlayerInfo[playerid][pNames], PlayerInfo[params[0]][pNames], data, unbandate, params[2]);
	mysql_tquery(connects, stringer1);
