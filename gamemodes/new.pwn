publics: Moderator(playerid,giveplayerid[],level)
{
	new querka[2], statics[228];
	cache_get_data(querka[0],querka[1]);
	if(querka[0] == 1)
	{
		cache_get_field_content(0, "level", statics);
		if(strval(statics) != 0)
		{
			if(level == 0)
			{
				format(stringer, MAX_SQL, "DELETE FROM `admin` WHERE `name`='%s'", giveplayerid);
				mysql_tquery(connects, stringer);
				format(stringer, MAX_SQL, "UPDATE `accounts` SET `bAdmin` = '0' WHERE `Name` = '%s'",giveplayerid);
				mysql_tquery(connects, stringer);
				format(stringer, SMALL_SIZE, "%s успешно удален из списка модераторов", giveplayerid);
				SendClientMessage(playerid, COLOR_LIGHTRED, stringer);
			}
			else
			{
				format(statics, MAX_SQL, "UPDATE `admin` SET  `level` = '%d' WHERE `name` = '%s' LIMIT 1", level,giveplayerid);
				mysql_tquery(connects, statics);
				format(stringer, MAX_STR, "Уровень администратирования у %s изменен на %d", giveplayerid, level);
				SendClientMessage(playerid, COLOR_GREEN, stringer);
			}
		}
	}
	else
	{
		if(level == 0) return SendClientMessage(playerid,-1,"Ошибка: игрок не администратор.");
		new years, months, days,data[32];
		getdate(years, months, days);
		format(data, 32, "%02d.%02d.%04d", days, months, years);
		format(stringer, BIG_SIZE, "INSERT INTO `admin` (`Name`, `level`, `LastCon`, `Postavil`) VALUES ('%s', %d, '%s', '%s')", giveplayerid, level, data, PlayerInfo[playerid][pNames]);
		mysql_tquery(connects, stringer);
		format(stringer, MAX_STR, "%s успешно добавлен в список модераторов. Уровень полномочий %d", giveplayerid, level);
		SendClientMessage(playerid, COLOR_GREEN, stringer);
	}
	return true;
}

